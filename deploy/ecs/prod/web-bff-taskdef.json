{
  "family": "homesite-webbff",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::373268394585:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "web-bff",
      "image": "373268394585.dkr.ecr.us-east-1.amazonaws.com/homesite/web-bff-api:PLACEHOLDER",
      "essential": true,
      "portMappings": [
        { "containerPort": 8080, "protocol": "tcp" }
      ],
      "environment": [],
      "secrets": [
        {
          "name": "Cors__AllowedOrigins__0",
          "valueFrom": "arn:aws:ssm:us-east-1:373268394585:parameter/homesite/prod/webbff/Cors__AllowedOrigins__0"
        },
        {
          "name": "Jwt__Audience",
          "valueFrom": "arn:aws:ssm:us-east-1:373268394585:parameter/homesite/prod/webbff/Jwt__Audience"
        },
        {
          "name": "Jwt__Issuer",
          "valueFrom": "arn:aws:ssm:us-east-1:373268394585:parameter/homesite/prod/webbff/Jwt__Issuer"
        },
        {
          "name": "Jwt__Key",
          "valueFrom": "arn:aws:ssm:us-east-1:373268394585:parameter/homesite/prod/webbff/Jwt__Key"
        },
        {
          "name": "Jwt__RequireHttpsMetadata",
          "valueFrom": "arn:aws:ssm:us-east-1:373268394585:parameter/homesite/prod/webbff/Jwt__RequireHttpsMetadata"
        },
        {
          "name": "Services__Auth",
          "valueFrom": "arn:aws:ssm:us-east-1:373268394585:parameter/homesite/prod/webbff/Services__Auth"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/homesite-webbff-prod",
          "awslogs-create-group": "true",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
